{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "biome:ci": {
      "dependsOn": ["^biome:ci"]
    },
    "biome:check": {
      "dependsOn": ["^biome:check"]
    },
    "biome:write": {
      "dependsOn": ["^biome:write"]
    },
    "ts": {
      "dependsOn": ["^ts"]
    },
    "prisma:generate": {
      "dependsOn": ["^prisma:generate"],
      "cache": false
    },
    "dev": {
      "dependsOn": ["prisma:generate"],
      "cache": false,
      "persistent": true
    }
  }
}
